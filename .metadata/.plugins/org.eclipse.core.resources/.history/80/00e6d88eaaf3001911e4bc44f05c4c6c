package com.array;

public class Repl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 for(int i=1; i<=5; i++)
			    
		    {
		     
		      for(int j=1; j<i; j++){
		    	  for(int k=4; k>=1; k--) {
		    		  
		    		  System.out.print(" ");
		    	  }
		        System.out.print(i);
		      }
		      System.out.println();
		    }
	}

}
