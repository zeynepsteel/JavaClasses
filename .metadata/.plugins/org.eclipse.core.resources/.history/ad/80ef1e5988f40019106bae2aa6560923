package com.array;

public class Quiz {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int day = 7;

		switch(day)
		{
		case 1:
		System.out.println("Monday");
		case 2:
		System.out.println("Tuesday");
		case 3:
		System.out.println("Wednesday");
		case 4:
		System.out.println("Thursday");
		case 5:
		System.out.println("Friday");
		}
	}

}
